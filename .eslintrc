{
    "extends": [
        "airbnb",
        "react-app",
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": [
            "error",
            {
                "extensions": [".js", ".jsx"]
            }
        ]
    }
}
